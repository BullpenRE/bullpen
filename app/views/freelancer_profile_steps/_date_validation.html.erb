<script>
    $('.btn-primary').on('click', (e) => {
        const start_date = new Date($('#startYear').val(), ($('#startMonth').val() - 1));
        const end_date = new Date($('#endYear').val(), ($('#endMonth').val() - 1));
        const alertElement = document.getElementById("alertWarning");

        debugger;

        if (!$('#currentJob').prop('checked') && (start_date > end_date)) {
            alertElement.classList.remove('d-none');
            alertElement.classList.add('d-block');
            e.preventDefault();
            e.stopPropagation();
        }
    });
</script>